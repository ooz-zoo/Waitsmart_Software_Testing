<?xml version="1.0" encoding="utf-8"?>
<!-- res/drawable/rounded_rectangle_background.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#FFFFFF" /> <!-- Set the background color of the rounded rectangle -->
    <corners android:radius="12dp" /> <!-- Set the corner radius to make it rounded -->
    <stroke
        android:width="1dp"
        android:color="@color/black" /> <!-- Set the stroke color and width, or remove this line if no stroke is needed -->
</shape>
